<BorderPane prefHeight="580.0" prefWidth="750.0" style="-fx-background-color: aec6cf;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainmenu.MainScreenController">
